# Environment variables template
# Copy this file to .env and fill in your values
# DO NOT commit .env file to version control

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id

# Athena Configuration
ATHENA_DATABASE=cms_open_payments
ATHENA_TABLE=general_payments
ATHENA_OUTPUT_BUCKET=cms-anomaly-detection-athena-results

# SageMaker Configuration
SAGEMAKER_ENDPOINT=cms-anomaly-endpoint-YYYYMMDD-HHMMSS

# GitHub Configuration
GITHUB_REPOSITORY_URL=https://github.com/your-org/your-repo
GITHUB_ACCESS_TOKEN=ghp_your_personal_access_token

# Terraform Backend Configuration
TERRAFORM_STATE_BUCKET=cms-anomaly-detection-terraform-state
TERRAFORM_STATE_KEY=terraform.tfstate
TERRAFORM_LOCK_TABLE=terraform-state-lock

# Application Configuration
ENVIRONMENT=prod
PROJECT_NAME=cms-anomaly-detection
